<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns="http://www.company.cn/routes"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.company.cn/routes /META-INF/routes.xsd">
    <!-- ==================== -->
    <!--          事件        -->
    <!-- ==================== -->
    <!-- 关注/取消关注 -->
    <route msgType="event" event="subscribe" bean="subscribeBLogic"/>
    <route msgType="event" event="unsubscribe" bean="unsubscribeBLogic"/>
    <!-- 菜单 -->
    <route msgType="event" event="VIEW" bean="viewBLogic"/>
    <route msgType="event" event="CLICK" eventKey="click_1" bean="clickBLogic"/>
    <!-- ==================== -->
    <!--         消息         -->
    <!-- ==================== -->
    <!-- 文本 -->
    <route msgType="text" bean="textMsgBLogic"/>
</routes>